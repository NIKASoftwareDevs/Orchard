<link href="//maxcdn.bootstrapcdn.com/font-awesome/4.3.0/css/font-awesome.min.css" rel="stylesheet">
<script>
    function runSearch() {
        window.location = 'Search?culture=en-US&q=' + encodeURIComponent($('#main-search').val());
    }
    $(function () {
        $('#main-search').keydown(function (event) {
            if (event.keyCode == 13) {
                runSearch();
            }
        });

        $('#main-search-button').click(function (event) {
            runSearch();
        });
    });
</script>

<div class="container-fluid">
    <div class="col-md-4">
        <a href='/'>
            <img src="~/Themes/wctk/Styles/images/CIDM/logo_lrg.jpg" />
        </a>
    </div>
    <div class="col-md-8">
        <div class="row row-search">
            <div class="col-md-offset-7 col-md-5 col-xs-offset-0 col-xs-12">
                <div class="form-horizontal">
                    <div class="form-group">
                        <div class="input-group">
                            <input id="main-search" type="text" class="form-control" placeholder="Search for...">
                            <span class="input-group-btn">
                                <button id="main-search-button" class="btn btn-default" type="button"><i class="fa fa-search"></i></button>
                            </span>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <div class="row">
            <div class="col-xs-12">
                <div id="main-nav" class="navbar navbar-inverse hidden-print" role="navigation">
                    <div class="navbar-header">
                        <button type="button" class="navbar-toggle" data-toggle="collapse" data-target="#main-navbar-collapse">
                            <span class="sr-only">Toggle navigation</span>
                            <span class="fa fa-bars"></span>
                            <span>Main Menu</span>
                        </button>
                    </div>
                    <div class="collapse navbar-collapse" id="main-navbar-collapse">
                        <ul class="nav navbar-nav">
                            <li id="nav-home"><a href="/">Home</a></li>
                            <li>
                                <a href="#" class="dropdown-toggle" data-toggle="dropdown">Knowledge Center<b class="caret"></b></a>
                                <ul class="dropdown-menu multi-level">
                                    <li><a href="/repository">Resources/Documents</a></li>
                                </ul>
                            </li>
                            <li>
                                <a href="#" class="dropdown-toggle" data-toggle="dropdown">World-Class ToolKit<b class="caret"></b></a>
                                <ul class="dropdown-menu multi-level">
                                    <li><a href="/world-class-toolkit/case-studies/">Case Studies</a></li>
                                    <li><a href="https://wcftoolkitdev.nika-ae.com/CIDM/History">CIDM Archive</a></li>
                                    <li><a href="/world-class-toolkit/mhs-facilities-design-guidelines-criteria-policy/">Design Guidelines, Criteria &amp; Policy</a></li>
                                    <li><a href="/world-class-toolkit/post-occupancy-evaluation-overview/">Post-Occupancy Evaluation</a></li>
                                    <li><a href="/project">Project Tool</a></li>
                                    <li><a href="/checklist">World-Class Checklist</a></li>
                                    <li><a href="/world-class-toolkit/world-class-checklist-reports/">World-Class Checklist Reports</a></li>
                                </ul>
                            </li>
                            <li><a href="/world-class-toolkit/mhs-facilities-analytics/">Executive Dashboard</a></li>
                            <li>
                                <a href="#" class="dropdown-toggle" data-toggle="dropdown">Contact Us<b class="caret"></b></a>
                                <ul class="dropdown-menu multi-level">
                                    <li><a href="/contact-us/frequently-asked-questions/">Frequently Asked Questions</a></li>
                                    <li><a href="/contact-us/website-notices-policies-disclaimers/">Website Notices, Policies &amp; Disclaimers</a></li>
                                    <li><a href="/contact-us/how-to-use-this-site/">How To Use This Site</a></li>
                                    <li><a href="/contact-us/support-availability/">Support Availability</a></li>
                                </ul>
                            </li>
                        </ul>
                        <ul class="nav navbar-nav navbar-right">
                            @if (Request.IsAuthenticated && User.Identity is System.Security.Principal.GenericIdentity)
                            {
                                var Claims = new List<System.Security.Claims.Claim>((User.Identity as System.Security.Principal.GenericIdentity).Claims);
                                <li>
                                    <a href="https://facilities.health.mil/Account">@(Claims[1].Value)</a>
                                </li>

                                if (Claims[2].Value == "True")
                                {
                                    <li>
                                        <a href="#" class="dropdown-toggle" data-toggle="dropdown">Admin<b class="caret"></b></a>
                                        <ul class="dropdown-menu multi-level">
                                            <li><a href="https://wcftoolkitdev.nika-ae.com/Admin">View Users</a></li>
                                            <li><a href="https://wcftoolkitdev.nika-ae.com/AdminCompose">"Compose Email</a></li>
                                            <li><a href="https://wcftoolkitdev.nika-ae.com/Admin/SupportEmail">"Update Email</a></li>
                                            <li><a href="https://wcftoolkitdev.nika-ae.com/Admin/Reindex">"Rebuild Document Index</a></li>
                                        </ul>
                                    </li>
                                }

                                <li>
                                    <a href="~/Users/Account/Logoff">Log Off</a>
                                </li>
                            }
                            else
                            {
                                <li>
                                    <a href="~/Users/Account/LogOn">Log In</a>
                                </li>
                            }
                        </ul>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
<div class="container-fluid body-content-container">
    @Display(Model.Navigation)
    <div class="body-content" style="position:relative">
        @Display(Model.Content)
