<script>
    function runSearch() {
        window.location = 'Search?culture=en-US&q=' + encodeURIComponent($('#main-search').val());
    }
    $(function () {
        $('#main-search').keydown(function (event) {
            if (event.keyCode == 13) {
                runSearch();
            }
        });

        $('#main-search-button').click(function (event) {
            runSearch();
        });
    });
</script>

<div class="container-fluid">
    <div class="col-md-4">
        <a href='/'>
            <img src="~/Themes/wctk/Styles/images/CIDM/logo_lrg.jpg" />
        </a>
    </div>
    <div class="col-md-8">
        <div class="row row-search">
            <div class="col-md-offset-7 col-md-5 col-xs-offset-0 col-xs-12">
                <div class="form-horizontal">
                    <div class="form-group">
                        <div class="input-group">
                            <input id="main-search" type="text" class="form-control" placeholder="Search for...">
                            <span class="input-group-btn">
                                <button id="main-search-button" class="btn btn-default" type="button"><i class="fa fa-search"></i></button>
                            </span>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <div class="row">
            <div class="col-xs-12">
                <div id="main-nav" class="navbar navbar-inverse hidden-print" role="navigation">
                    <div class="navbar-header">
                        <button type="button" class="navbar-toggle" data-toggle="collapse" data-target="#main-navbar-collapse">
                            <span class="sr-only">Toggle navigation</span>
                            <span class="fa fa-bars"></span>
                            <span>Main Menu</span>
                        </button>
                    </div>
                    <div class="collapse navbar-collapse" id="main-navbar-collapse">
                        @Html.Partial("_NavbarLinks")
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
<div class="container-fluid body-content-container">
    @Display(Model.Navigation)
    <div class="body-content" style="position:relative">
        @Display(Model.Content)
    </div>
    <footer>
        <p>
            <a href="/website-notices-policies-disclaimers">Website Notices, Policies &amp; Disclaimers</a> |
            <a href="/document-posting-restrictions">Document Posting Restrictions</a> |
            <a href="http://health.mil/Military-Health-Topics/Privacy-and-Civil-Liberties">FOIA</a> |
            <a href="/sitemap">Site Map</a> |
            <a href="/contact-us">Contact Us</a>
        </p>
    </footer>
</div>
